{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
  "scripts": {
    "db:teardown": "./infra_scripts/db/teardown.sh",
    "db:setup": "./infra_scripts/db/setup.sh",
    "db:reset": "npm run db:teardown -- -y && npm run db:setup -- -y",
    "docker:check": "bash ./infra_scripts/docker/check_conflicts.sh",
    "docker:up:local": "bash ./infra_scripts/docker/check_conflicts.sh && docker compose -f docker-compose.db.yml -f docker-compose.yml -f docker-compose.override.yml --profile local up",
    "docker:up:remote": "bash ./infra_scripts/docker/check_conflicts.sh && docker compose -f docker-compose.db.yml -f docker-compose.yml -f docker-compose.remote.yml --profile remote up",
    "docker:up:prod": "bash ./infra_scripts/docker/check_conflicts.sh && docker compose -f docker-compose.db.yml -f docker-compose.yml -f docker-compose.prod.yml --profile prod up --build",
    "docker:forceUp": "bash ./infra_scripts/docker/force_up.sh",
    "docker:forceUp:local": "npm run docker:forceUp -- local",
    "docker:forceUp:remote": "npm run docker:forceUp -- remote",
    "docker:forceUp:prod": "npm run docker:forceUp -- prod",
    "strapi:build": "strapi build",
    "strapi:console": "strapi console",
    "strapi:deploy": "strapi deploy",
    "strapi:dev": "strapi develop",
    "strapi:develop": "strapi develop",
    "strapi:develop:log": "npm run strapi:develop 2>&1 | tee logs/strapi-all.log",
    "strapi:start": "strapi start",
    "strapi:strapi": "strapi",
    "strapi:upgrade": "npx @strapi/upgrade latest",
    "strapi:upgrade:dry": "npx @strapi/upgrade latest --dry",
    "strapi:upgrade:luke": "npm install @strapi/strapi@latest @strapi/plugin-users-permissions@latest && npm run strapi:build",
    "test:logger": "NODE_ENV=development node tests/logger.test.js"
  },
  "dependencies": {
    "@strapi/logger": "^5.12.3",
    "@strapi/plugin-users-permissions": "5.11.3",
    "@strapi/provider-upload-aws-s3": "^5.11.3",
    "@strapi/strapi": "^5.11.3",
    "axios": "^1.8.4",
    "better-sqlite3": "^11.3.0",
    "pg": "^8.14.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0",
    "vite": "^5.4.15",
    "winston": "^3.17.0"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "uuid": "fca9e32f-be1f-4a52-a873-ef33f8e58c3a"
  }
}
