{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
  "scripts": {
    "db:teardown": "./infra_scripts/db/teardown.sh",
    "db:setup": "./infra_scripts/db/setup.sh",
    "db:reset": "npm run db:teardown -- -y && npm run db:setup -- -y",


    "docker:check": "./infra_scripts/docker/check_conflicts.sh",
    "docker:up": "./infra_scripts/docker/check_conflicts.sh && docker compose up",
    "docker:forceUp": "./infra_scripts/docker/force_up.sh",


    "strapi:build": "strapi build",
    "strapi:console": "strapi console",
    "strapi:deploy": "strapi deploy",
    "strapi:dev": "strapi develop",
    "strapi:develop": "strapi develop",
    "strapi:start": "strapi start",
    "strapi:strapi": "strapi",
    "strapi:upgrade": "npx @strapi/upgrade latest",
    "strapi:upgrade:dry": "npx @strapi/upgrade latest --dry",

    "strapi:upgrade:luke": "npm install @strapi/strapi@latest @strapi/plugin-cloud@latest @strapi/plugin-users-permissions@latest && npm run build"
  },
  "dependencies": {
    "@strapi/plugin-cloud": "5.11.3",
    "@strapi/plugin-users-permissions": "5.11.3",
    "@strapi/provider-upload-aws-s3": "^5.11.3",
    "@strapi/strapi": "^5.11.3",
    "axios": "^1.8.4",
    "better-sqlite3": "^11.3.0",
    "pg": "^8.14.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "uuid": "fca9e32f-be1f-4a52-a873-ef33f8e58c3a"
  }
}
